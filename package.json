{
  "name": "readme-gpt",
  "version": "1.0.0",
  "description": "AI-powered README generator CLI tool",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "readme-gpt": "./dist/bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node -r dotenv/config dist/bin/index.js",
    "dev": "node -r dotenv/config node_modules/.bin/ts-node src/bin/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "readme",
    "ai",
    "cli",
    "generator",
    "github",
    "documentation",
    "gpt"
  ],
  "author": "Anshul Kapoor",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "dotenv": "^16.5.0",
    "fs-extra": "^11.1.1",
    "openai": "^4.0.0",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
